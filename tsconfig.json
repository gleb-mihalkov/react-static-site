{
  "compilerOptions": {
    "lib": [
      "esnext",
      "dom",
    ],
    "module": "esnext",
    "target": "es5",
    "jsx": "react",
    "moduleResolution": "node",
    "baseUrl": "src",
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "resolveJsonModule": true,
    "skipLibCheck": true
  }
}
